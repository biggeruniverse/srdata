makeanim alt_melee_move_2 clip/melee_2.clip 0 0 45
makeanim alt_melee_move_3 clip/melee_3.clip 0 0 45
makeanim alt_melee_move_4 clip/melee_4.clip 0 0 45

makeanim block clip/block.clip

//general character animation

makeanim walk_left clip/sidestep_left.clip 0 0 25 1
makeanim walk_right clip/sidestep_right.clip 0 0 25 1
makeanim walk_fwd clip/run_fwd.clip 0 0 20 1
