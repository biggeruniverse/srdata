// Boom ring, delay was 150 150
// growth has low range so particles are additive
vfEdit 0
vfSet visclass "particles"
vfSet flags -parent_pos -parent_vel -param_location -param_scale -param_count -grow_shrink_linear 
vfSet shader "/textures/effects/stars/1_nl_ringbg.tga"
vfSet size 0.010000 0.010000
vfSet growth 118.000000 120.000000
vfSet alpha 1.000000 1.000000
vfSet fade 1.000000 1.000000
