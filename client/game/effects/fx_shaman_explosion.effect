vfSet red 1.000000 1.000000
vfSet green 1.000000 1.000000
vfSet blue 1.000000 1.000000
vfSet redfade -0.500000 -0.500000
vfSet greenfade -1.000000 -1.000000
vfSet bluefade 0.000000 0.000000
vfSet delay 0 0
vfSet lifetime 170 200
vfSet tilelength 0.000000
vfSet style "point"
vfSet model ""
vfSet skin ""
vfSet count 8 12
