//first person view weapon animations
makeanim idle natuward.clip 0 1 
makeanim wpstate_switch natuward.clip
makeanim wpstate_charge natuward.clip 
makeanim wpstate_spinup natuward.clip 
makeanim wpstate_spindown natuward.clip
makeanim wpstate_overheat natuward.clip
makeanim wpstate_fire natuward.clip 0 20

animevent wpstate_fire 0 "sound /sound/human/weapons/regen_throw.wav"
animevent wpstate_charge 9 "sound /sound/human/weapons/regen_pull.wav"
