set mat_uAmplitude 0.020
set mat_uPhase 0.0
set mat_uspeed 0.8
set mat_vfunc SIN_PAN
set mat_vAmplitude 0.010
set mat_vPhase 90
set mat_vspeed 1.1
set mat_vOffset -0.007
set mat_alphaCutoff 0.8
endMatPass
